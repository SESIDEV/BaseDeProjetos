@using System.Globalization
@using BaseDeProjetos.Models.ViewModels
@model ParticipacaoViewModel
@{
    var cultura = new CultureInfo("pt-BR");
}

<div class="modal fade" id="modalProspeccoes" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="modalProspeccoes" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title fs-5" id="modalProspeccoes">Prospecções do Pesquisador</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="spinner-border text-success" id="loadingProspeccoes"></div>
                <div class="d-flex flex-wrap gap-4" id="corpoProspeccoes">
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn app-btn-secondary" data-bs-dismiss="modal">Fechar</button>
            </div>
        </div>
    </div>
</div>
