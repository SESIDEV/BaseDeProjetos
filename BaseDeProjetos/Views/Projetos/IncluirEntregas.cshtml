@{
}

<div class="card" id="contact" role="tabpanel" aria-labelledby="contact-tab">
    <div class="card-body">
        <h5 class="card-title">Carregar lista de Projetos </h5>
        <form class="card-text" enctype="multipart/form-data" action="Entregas/CarregarEntregas" method="POST">
            <div class="form-group">
                <span>
                    <label>Upload de arquivo</label>
                </span>
                <div class="custom-file">
                    <input type="file" class="custom-file-input" name="files" id="customFile">
                    <label class="custom-file-label" for="customFile">Choose file</label>
                </div>
            </div>
            <input class="btn btn-dark" type="submit" value="Upload" />
        </form>
    </div>
</div>