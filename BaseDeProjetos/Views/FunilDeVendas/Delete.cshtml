@model BaseDeProjetos.Models.Prospeccao

@{
    ViewData["Title"] = "Delete";
}

<h1>Deletar</h1>
<br />
<h3>Você realmente quer deletar essa prospecção ?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TipoContratacao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TipoContratacao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.LinhaPequisa)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LinhaPequisa)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Deletar" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-outline-dark" asp-route-casa="@ViewData["Area"]">Voltar</a>
    </form>
</div>