@model IEnumerable<BaseDeProjetos.Models.Producao>
@addTagHelper *, BaseDeProjetos.Models.Producao

@{
	SelectList ListProjetos = ViewData["Projetos"] as SelectList;
	SelectList ListEmpresas = ViewData["Empresas"] as SelectList;

	ViewData["Title"] = "Gestão do Conhecimento";
	IEnumerable<BaseDeProjetos.Models.Producao> ListaProducoes = ViewData["ListaProducoes"] as IEnumerable<BaseDeProjetos.Models.Producao>;
}
<div class="app-wrapper">
	<div class="app-content pt-3 p-md-3 p-lg-4">
		<div class="container-xl">
			<div class="row g-3 mb-4 align-items-center justify-content-between">
				<div class="col-auto">
					<h1 class="app-page-title mb-0">Gestão do Conhecimento</h1>
				</div>
				<div class="col-auto">
						<div class="page-utilities">
						<div class="row g-2 justify-content-start justify-content-md-end align-items-center">
							<div class="col-auto">
								<form asp-controller="Producao" asp-action="Index" method="get" class="docs-search-form row gx-1 align-items-center">
									<div class="col-auto">
										<input type="text" id="search-docs" name="SearchString" class="form-control search-docs" value='@ViewData["CurrentFilter"]' placeholder="Digite o nome da publicação">
									</div>
									<div class="col-auto">
										<button type="submit" class="btn app-btn-secondary">Procurar</button>
									</div>
								</form>

							</div><!--//col-->
							<div class="col-auto">
								<select class="form-select w-auto" id="ano" name="ano" onchange="updateLink()">
										<option selected="" value="Todos">Ano</option>
										<option value="2022">2022</option>
										<option value="2021">2021</option>
										<option value="2020">2020</option>
										<option value="2019">2019</option>
										<option value="2018">2018</option>

								</select>
							</div>
								<div class="col-auto">
									<form asp-action="Create">
									<div asp-validation-summary="ModelOnly" class="text-danger"></div>
									<div class="modal fade" id="modalGestaoConhecimentoToggle" aria-hidden="true" aria-labelledby="modalGestaoConhecimentoToggleLabel" tabindex="-1">
										<div class="modal-dialog modal-dialog-centered">
											<div class="modal-content">
												<div class="modal-header">
													<h1 class="modal-title fs-5" id="modalGestaoConhecimentoToggleLabel">Adicionar Produção</h1>
													<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
												</div>
												<div class="modal-body">
													<div class="form-group">
														<partial name="Create" view-data="@ViewData" />
													</div>
												</div>												
											</div>
										</div>
									</div>									
									<a class="btn app-btn-primary" data-bs-toggle="modal" href="#modalGestaoConhecimentoToggle" role="button">Adicionar produção</a>
								</form>
								</div>
							
						</div><!--//row-->
					</div><!--//table-utilities-->
				</div><!--//col-auto-->
			</div><!--//row-->

			<div class="row g-4">
				@foreach (var item in ListaProducoes) {
				<div class="col-6 col-md-4 col-xl-3 col-xxl-3">
					<div class="app-card app-card-doc shadow-sm h-100">
						<div class="app-card-thumb-holder p-3">
							<div class="app-card-thumb">
									<img class="thumb-image" src="@Html.DisplayFor(modelItem => item.Imagem)" alt="imagem">
