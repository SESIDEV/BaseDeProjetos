										<li><span class="text-muted">Status:</span> @Html.DisplayFor(modelItem => item.StatusPub)</li>										
										<li><span class="text-muted">Publicado em:</span> @Html.DisplayFor(modelItem => item.Data.Month)/@Html.DisplayFor(modelItem => item.Data.Year)</li>
