@page
@model BaseDeProjetos.Views.Home.LoggedInHomeModel
@{
    decimal receita = 4722542.00M;
    decimal investimento = 3631437.00M;
    decimal despesas = 2558188M;
    decimal sustentatiblidade = receita / despesas;
}
<style>
    #sustentabilidade {
        background-image: url("https://ogimg.infoglobo.com.br/sociedade/24483206-8b9-2d9/FT1086A/652/IC_Multiplex-Covid-19.jpg");
        background-image: linear-gradient(to bottom, rgba(255,255,255,0.5) 0%,rgba(255,255,255,0.4) 100%), url("https://ogimg.infoglobo.com.br/sociedade/24483206-8b9-2d9/FT1086A/652/IC_Multiplex-Covid-19.jpg");
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
    }

    #seguranca {
        background-image: url("https://conect.online/wp-content/uploads/2018/10/243181-conheca-o-campo-de-trabalho-para-tecnico-em-seguranca-do-trabalho.jpg");
        background-image: linear-gradient(to bottom, rgba(255,255,255,0.5) 0%,rgba(255,255,255,0.4) 100%), url("https://conect.online/wp-content/uploads/2018/10/243181-conheca-o-campo-de-trabalho-para-tecnico-em-seguranca-do-trabalho.jpg");
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
    }
</style>
<div class="row">
    <div class="col-md-2">
        <div class="row">
            <button class="col-12 card" data-toggle="collapse" data-target="#collapse_ISIQV" aria-expanded="true" aria-controls="collapse_ISIQV">
                <!--
                <img src="https://jornal.usp.br/wp-content/uploads/20170704_00_quimica_verde.jpg" alt="qv" class="card-img-top" />
                    -->
                <div class="card-body">
                    <h5 class="text-center card-text text-info">ISI-QV</h5>
                </div>
            </button>
            <div class="col-12 collapse" id="collapse_ISIQV">
                <ul class="list-group-flush">
                    <li class="list-group-item">
                        <a asp-action="Index" asp-controller="FunilDeVendas" asp-route-casa="ISIQV">
                            Funil de Vendas
                        </a>
                    </li>
                    <li class="list-group-item">
                        <a asp-action="Index" asp-controller="Projetos" asp-route-casa="ISIQV">
                            Base de Projetos
                        </a>
                    </li>
                    <li class="list-group-item disabled">
                        Gestão do conhecimento
                        <span class="badge badge-warning badge-pill">!</span>
                    </li>
                    <li class="list-group-item disabled">
                        Satisfação do Cliente
                        <span class="badge badge-warning badge-pill">!</span>
                    </li>
                    <li class="list-group-item disabled">
                        Segurança e Qualidade
                        <span class="badge badge-warning badge-pill">!</span>
                    </li>
                </ul>
            </div>

        </div>
        <div class="row">
            <button class="col-12 card" data-toggle="collapse" data-target="#collapse_ISIII" aria-expanded="true" aria-controls="collapse_ISIII">
                <!--
                                <img src="https://www.klium.com/blog/wp-content/uploads/2020/01/shutterstock_538992253.jpg" alt="qv" class="card-img-top" />
                -->
                <div class="card-body">
                    <h5 class="text-center card-title text-info">ISI-II</h5>
                </div>
            </button>
            <div class="col-12 collapse" id="collapse_ISIII">
                <ul class="list-group-flush">
                    <li class="list-group-item">
                        <a asp-action="Index" asp-controller="FunilDeVendas" asp-route-casa="ISIII">
                            Funil de Vendas
                        </a>
                    </li>
                    <li class="list-group-item">
                        <a asp-action="Index" asp-controller="Projetos" asp-route-casa="ISIII">
                            Base de Projetos
                        </a>
                    </li>
                    <li class="list-group-item disabled">
                        Gestão do conhecimento
                        <span class="badge badge-warning badge-pill">!</span>
                    </li>
                    <li class="list-group-item disabled">
                        Satisfação do Cliente
                        <span class="badge badge-warning badge-pill">!</span>
                    </li>
                    <li class="list-group-item disabled">
                        Segurança e Qualidade
                        <span class="badge badge-warning badge-pill">!</span>
                    </li>
                </ul>
            </div>

        </div>
        <div class="row">
            <button class="col-12 card" data-toggle="collapse" data-target="#collapse_CISHO" aria-expanded="true" aria-controls="collapse_CISHO">
                <!--
                                <img src="data:image/jpeg;base64," alt="qv" class="card-img-top" />
                -->
                <div class=" card-body">
                    <h5 class="text-center text-info">CIS-HO</h5>
                </div>
            </button>
            <div class="col-12 collapse" id="collapse_CISHO">
                <ul class="list-group-flush">
                    <li class="list-group-item">
                        <a asp-action="Index" asp-controller="FunilDeVendas" asp-route-casa="CISHO">
                            Funil de Vendas
                        </a>
                    </li>
                    <li class="list-group-item">
                        <a asp-action="Index" asp-controller="Projetos" asp-route-casa="CISHO">
                            Base de Projetos
                        </a>
                    </li>
                    <li class="list-group-item disabled">
                        Gestão do conhecimento
                        <span class="badge badge-warning badge-pill">!</span>
                    </li>
                    <li class="list-group-item disabled">
                        Satisfação do Cliente
                        <span class="badge badge-warning badge-pill">!</span>
                    </li>
                    <li class="list-group-item disabled">
                        Segurança e Qualidade
                        <span class="badge badge-warning badge-pill">!</span>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="col-md-10">
        <div class="card">
            <div class="card-header text-center"> <h5>Bem vindo ao sistema de Gestão de Inovação - SGI</h5></div>
            <div class="card-body row">
                <div class="col-8">
                    <ul class="col-12 list-group-flush">
                        <li class="list-group-item">
                            <span class="font-weight-bold"> Receita:</span>
                            <a href="#" class="badge badge-success">@receita.ToString("C2")</a>
                        </li>
                        <li class="list-group-item">
                            <span class="font-weight-bold"> Investimento:</span>
                            <a href="#" class="badge badge-light">@investimento.ToString("C2")</a>
                        </li>
                        <li class="list-group-item">
                            <span class="font-weight-bold"> Despesas:</span>
                            <a href="#" class="badge badge-danger">@despesas.ToString("C2")</a>
                        </li>
                    </ul>
                    <div class="col card">
                        <div class="card-body">
                            <div class="card-title">Projetos</div>
                            <partial name="graph_projetos" />
                            <div class="card-text"> </div>
                        </div>
                    </div>
                    <div class="col card">
                        <div class="card-body">
                            <div class="card-title">Funil de Vendas</div>
                            <partial name="graph_funil" />
                            <div class="card-text"> </div>
                        </div>
                    </div>

                </div>
                <div class="col-4 row">
                    <div id="sustentabilidade" class="jumbotron col-12">
                        <center>
                            <span class="text-uppercase">Sustentabilidade</span>
                            <hr />
                            <h3 class="font-weight-bolder">
                                @sustentatiblidade.ToString("P2")
                            </h3>

                            <span class="font-weight-lighter">das despesas são cobertas por receita</span>
                        </center>
                    </div>
                    <div id="seguranca" class="jumbotron col-12">
                        <center>
                            <span class="text-uppercase">Indice de Segurança e Qualidade</span>
                            <hr />
                            <h3 class="font-weight-bolder">
                                @{string iss = ((sustentatiblidade - 1) * 100).ToString("F0");
                                }
                                @iss
                            </h3>

                            <span class="font-weight-lighter">de um total de 100 pontos</span>
                        </center>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
