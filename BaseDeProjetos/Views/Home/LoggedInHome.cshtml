@model BaseDeProjetos.Views.Home.LoggedInHomeModel
@using BaseDeProjetos.Helpers
@using System.Text.Json;
@{

	int n_prosp_ativas = 0;
	decimal receita = 1;
	decimal despesas = 1;
	decimal investimento = 1;
	decimal sustentabilidade = 100;
	float quali = 1;
	List<LinhaPesquisa> linhaDePesquisa = new List<LinhaPesquisa>();
	string labelLinhaDePesquisa = "";
	string dadosLinhaDePesquisa = "";

	try
	{
		//decimal receita = (decimal)ViewBag.receita_isiqv + (decimal)ViewBag.receita_isiii + (decimal)ViewBag.receita_cisho;
		receita = (decimal)ViewBag.receita_total;
		investimento = (decimal)ViewBag.invest_total;
		despesas = (decimal)ViewBag.despesa_total;
		quali = (float)ViewBag.quali;
		n_prosp_ativas = (int)ViewBag.n_prosp;

		linhaDePesquisa = ViewBag.LinhaDePesquisa as List<LinhaPesquisa>;


		// Separação do query SQL em algumas variáveis para clareza
		var linhaDePesquisaDistintos = linhaDePesquisa.OrderByDescending(p => linhaDePesquisa.Where(k => k == p).Count()).Distinct();
		//dadosGraficoLinhaDePesquisa = linhaDePesquisaDistintos.Select(p => $"{{name: `{p.GetDisplayName().ToUpperInvariant()}`, y: {linhaDePesquisa.Where(k => k == p).Count()}}},").ToList();
		List<int> quantidades = linhaDePesquisaDistintos.Select(p => linhaDePesquisa.Where(k => k == p).Count()).ToList();
		List<string> linha = linhaDePesquisaDistintos.Select(p => $"`{p.GetDisplayName().ToUpperInvariant()}`").ToList();

		for (int i = 0; i < quantidades.Count(); i++)
		{
			labelLinhaDePesquisa += linha[i] + ",";
			dadosLinhaDePesquisa += quantidades[i] + ",";			
		}

        if (despesas > 0)
        {

            sustentabilidade = (investimento / despesas) * 100;
        }
        else
        {
            sustentabilidade = 100;
        }
    }

    catch (Exception erro)
    {
        Console.WriteLine(erro);
    }
}


