@model IEnumerable<BaseDeProjetos.Models.Maquina>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdentificacaoDaMaquina)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ManutencaoAnoAnterior)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorManutenCaoAnoAnterior)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OcupacaoMax)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OcupacaoAtual)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrecoBase)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustoHoraMaquina)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nome)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdentificacaoDaMaquina)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ManutencaoAnoAnterior)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ValorManutenCaoAnoAnterior)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OcupacaoMax)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OcupacaoAtual)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PrecoBase)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CustoHoraMaquina)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
