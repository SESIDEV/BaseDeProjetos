@model IEnumerable<BaseDeProjetos.Models.PesquisaProjeto>

@{
    ViewData["Title"] = "Index";
}
<partial name="Header" view-data="@ViewData" />

<div class="app-wrapper">
    <div class="app-content pt-3 p-md-3 p-lg-4">
        <div class="container-xl">

            <h1>Index</h1>

            <p>
                <a asp-action="Create">Create New</a>
            </p>

            <table class="table">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.ProjetoId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.ResultadoFinal)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.RepresentacaoTextualQuestionario)
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.ProjetoId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.ResultadoFinal)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.RepresentacaoTextualQuestionario)
                            </td>
                            <td>
                                <a asp-action="Edit" asp-route-id="@item.IdPesquisa">Edit</a> |
                                <a asp-action="Details" asp-route-id="@item.IdPesquisa">Details</a> |
                                <a asp-action="Delete" asp-route-id="@item.IdPesquisa">Delete</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>

        </div>
        <partial name="Footer" />
    </div>
